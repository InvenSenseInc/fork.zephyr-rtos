/*
 * Example configuration of a ICM42670S device on an Arduino I2C bus.
 *
 * Device address 0x77 is assumed. Your device may have a different
 * address; check your device documentation if unsure.
 */
&arduino_i2c {
	status = "okay";
	icm42670S@68 {
		compatible = "tdk,icm42670S";
		reg = <0x68>;
	};
};
